num_vec <- c(1.5, 2.0, 3.25, 4)
char_vec <- c("apple", "banana", "cherry")
log_vec <- c(TRUE, FALSE, TRUE, FALSE)

cat("Numeric vector:\n")
print(num_vec)
cat("Type:", class(num_vec), "\n\n")

cat("Character vector:\n")
print(char_vec)
cat("Type:", class(char_vec), "\n\n")

cat("Logical vector:\n")
print(log_vec)
cat("Type:", class(log_vec), "\n\n")
